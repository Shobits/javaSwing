<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.dao.ProvinceDao">
    <select id="selectProvinceList" resultType="org.example.domain.Province">
        select id, name
        from province
        order by id
    </select>
</mapper>